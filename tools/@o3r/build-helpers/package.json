{
  "name": "@o3r/build-helpers",
  "version": "0.0.0-placeholder",
  "description": "Build helpers",
  "type": "module",
  "private": true,
  "keywords": [
    "otter",
    "amadeus",
    "typescript"
  ],
  "files": [
    "scripts/*.mjs"
  ],
  "bin": {
    "clear-package-json-dev-dependencies": "./scripts/clear-package-json-dev-dependencies.mjs",
    "cp-assets": "./scripts/cp-assets.mjs",
    "cp-build": "./scripts/cp-build.mjs",
    "cp-package-json": "./scripts/cp-package-json.mjs",
    "doc-links": "./scripts/doc-links.mjs",
    "generate-cjs-manifest": "./scripts/generate-cjs-manifest.mjs",
    "patch-package-json-exports": "./scripts/patch-package-json-exports.mjs",
    "patch-package-json-main": "./scripts/patch-package-json-main.mjs",
    "pr-labels": "./scripts/pr-labels.mjs",
    "prepare-doc-root-menu-template": "./scripts/prepare-doc-root-menu-template.mjs",
    "prepare-publish": "./scripts/prepare-publish.mjs",
    "update-doc-summary": "./scripts/update-doc-summary.mjs",
    "update-package": "./scripts/update-package.mjs"
  },
  "dependencies": {
    "globby": "^14.0.0",
    "minimist": "^1.2.6"
  },
  "peerDependencies": {
    "typescript": "~5.4.2"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "~18.0.0",
    "@nx/eslint-plugin": "~19.3.0",
    "@o3r/eslint-plugin": "workspace:^",
    "@stylistic/eslint-plugin-ts": "^2.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@typescript-eslint/utils": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-jest": "~28.6.0",
    "eslint-plugin-jsdoc": "~48.5.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "nx": "~19.3.0",
    "typescript": "~5.4.2"
  },
  "otter": {
    "versionHarmonize": {
      "ignore": [
        "globby"
      ]
    }
  }
}
