{
  "name": "@o3r/build-helpers",
  "version": "0.0.0-placeholder",
  "description": "Typescript plugin to copy assets to the outDir",
  "type": "module",
  "keywords": [
    "otter",
    "amadeus",
    "typescript"
  ],
  "files": [
    "scripts/*.mjs"
  ],
  "bin": {
    "cp-assets": "./scripts/cp-assets.mjs",
    "cp-build": "./scripts/cp-build.mjs",
    "cp-package-json": "./scripts/cp-package-json.mjs",
    "dispatch-package-json-fields": "./scripts/dispatch-package-json-fields.mjs",
    "patch-package-json-exports": "./scripts/patch-package-json-exports.mjs",
    "patch-package-json-main": "./scripts/patch-package-json-main.mjs"
  },
  "dependencies": {
    "globby": "^13.1.2",
    "minimist": "^1.2.6"
  },
  "peerDependencies": {
    "typescript": "~4.8.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "nx": "^15.3.3",
    "typescript": "~4.8.4"
  },
  "otter": {
    "versionHarmonize": {
      "ignore": [
        "globby"
      ]
    }
  }
}
