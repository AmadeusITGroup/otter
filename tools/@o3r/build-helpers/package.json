{
  "name": "@o3r/build-helpers",
  "version": "0.0.0-placeholder",
  "description": "Build helpers",
  "type": "module",
  "private": true,
  "keywords": [
    "otter",
    "amadeus",
    "typescript"
  ],
  "files": [
    "scripts/*.mjs"
  ],
  "bin": {
    "cp-assets": "./scripts/cp-assets.mjs",
    "cp-build": "./scripts/cp-build.mjs",
    "cp-package-json": "./scripts/cp-package-json.mjs",
    "doc-links": "./scripts/doc-links.mjs",
    "patch-package-json-exports": "./scripts/patch-package-json-exports.mjs",
    "patch-package-json-main": "./scripts/patch-package-json-main.mjs",
    "prepare-publish": "./scripts/prepare-publish.mjs",
    "update-doc-summary": "./scripts/update-doc-summary.mjs",
    "update-package": "./scripts/update-package.mjs"
  },
  "dependencies": {
    "globby": "^13.1.2",
    "minimist": "^1.2.6"
  },
  "peerDependencies": {
    "typescript": "~4.9.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "nx": "~15.8.0",
    "typescript": "~4.9.4"
  },
  "otter": {
    "versionHarmonize": {
      "ignore": [
        "globby"
      ]
    }
  }
}
