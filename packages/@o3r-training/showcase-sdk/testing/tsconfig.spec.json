{
  "extends": "../tsconfig.build",
  "compilerOptions": {
    "types": [
      "jest",
      "node"
    ],
    "esModuleInterop": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "incremental": true,
    "composite": true,
    "declaration": false,
    "declarationMap": false,
    "strict": false,
    "module": "ES2020",
    "target": "ES2020"
  },
  "include": [
    "../src/**/*.spec.ts",
    "../src/**/*.perf.ts"
  ]
}
