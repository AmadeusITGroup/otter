{
  "name": "@ama-sdk/core",
  "version": "0.0.0-placeholder",
  "description": "Digital API - SDK Core",
  "module": "dist/src/public_api.js",
  "esm2015": "dist/esm2015/public_api.js",
  "esm2020": "dist/src/public_api.js",
  "typings": "dist/src/public_api.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "module": "./dist/src/public_api.js",
      "esm2020": "./dist/src/public_api.js",
      "esm2015": "./dist/esm2015/public_api.js",
      "es2020": "./dist/cjs/public_api.js",
      "default": "./dist/cjs/public_api.js",
      "typings": "./dist/src/public_api.d.ts",
      "import": "./dist/src/public_api.js",
      "node": "./dist/cjs/public_api.js",
      "require": "./dist/cjs/public_api.js"
    },
    "./clients/*": {
      "module": "./dist/src/clients/*.js",
      "esm2020": "./dist/src/clients/*.js",
      "esm2015": "./dist/esm2015/clients/*.js",
      "es2020": "./dist/cjs/clients/*.js",
      "default": "./dist/cjs/clients/*.js",
      "typings": "./dist/src/clients/*.d.ts",
      "import": "./dist/src/clients/*.js",
      "node": "./dist/cjs/clients/*.js",
      "require": "./dist/cjs/clients/*.js"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-sdk-core",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs -q",
    "build:esm2015": "swc src -d dist/esm2015 -C module.type=es6 -q",
    "build:esm2020": "tsc -b tsconfig.build.json",
    "postbuild": "yarn cpy './package.json' dist && patch-package-json-main",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy '{collection,package}.json' dist",
    "build:cli": "tsc -b tsconfig.cli.json --pretty && yarn generate-cjs-manifest",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "prepare:publish": "prepare-publish ./dist"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.0",
    "tslib": "^2.5.3",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "~17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/common": "~17.0.2",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "~17.0.0",
    "chokidar": "^3.5.2",
    "cpy": "~10.1.0",
    "globby": "^11.1.0",
    "isomorphic-fetch": "^3.0.0",
    "minimist": "^1.2.6",
    "rxjs": "^7.8.1",
    "typescript": "~5.2.2"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@angular/cli": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@schematics/angular": {
      "optional": true
    },
    "isomorphic-fetch": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular-devkit/core": "~17.0.3",
    "@angular-devkit/schematics": "~17.0.3",
    "@angular-eslint/eslint-plugin": "~17.1.0",
    "@angular/common": "~17.0.4",
    "@angular/core": "~17.0.4",
    "@nx/eslint-plugin": "~17.1.1",
    "@nx/jest": "~17.1.1",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@schematics/angular": "~17.0.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.85",
    "@types/jest": "~29.5.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "chokidar": "^3.5.2",
    "copyfiles": "^2.4.1",
    "cpy": "~10.1.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "~27.6.0",
    "eslint-plugin-jsdoc": "~46.9.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "globby": "^11.1.0",
    "isomorphic-fetch": "~3.0.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "minimist": "^1.2.6",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "ts-jest": "~29.1.1",
    "ts-node": "~10.9.1",
    "type-fest": "^3.12.0",
    "typescript": "~5.2.2",
    "zone.js": "~0.14.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "schematics": "./collection.json",
  "bin": {
    "amasdk-clear-index": "./dist/cli/clear-index.cjs",
    "amasdk-files-pack": "./dist/cli/files-pack.cjs"
  }
}
