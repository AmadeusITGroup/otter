{
  "name": "@ama-sdk/core",
  "version": "0.0.0-placeholder",
  "description": "Digital API - SDK Core",
  "main": "dist/cjs/public_api.js",
  "module": "dist/src/public_api.js",
  "esm2015": "dist/esm2015/public_api.js",
  "esm2020": "dist/src/public_api.js",
  "typings": "dist/src/public_api.d.ts",
  "homepage": "https://dev.azure.com/AmadeusDigitalAirline/DES-SDKs/_git/sdk-tools",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "module": "./dist/src/public_api.js",
      "es2020": "./dist/src/public_api.js",
      "esm2020": "./dist/src/public_api.js",
      "es2015": "./dist/esm2015/public_api.js",
      "esm2015": "./dist/esm2015/public_api.js",
      "default": "./dist/src/public_api.js",
      "typings": "./dist/src/public_api.d.ts",
      "import": "./dist/src/public_api.js",
      "node": "./dist/cjs/public_api.js",
      "require": "./dist/cjs/public_api.js"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-sdk-core",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs -q",
    "build:esm2015": "swc src -d dist/esm2015 -C module.type=es6 -q",
    "build:esm2020": "tsc -b tsconfig.esm2020.json",
    "postbuild": "patch-package-json-main",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty",
    "prepare:publish": "dispatch-package-json-fields ./dist/package.json"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.11",
    "tslib": "^2.4.1",
    "uuid": "^8.3.2"
  },
  "peerDependenciesMeta": {
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "@angular/compiler": {
      "optional": true
    }
  },
  "peerDependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "~15.1.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.249",
    "@types/jest": "~28.1.2",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "globby": "^11.1.0",
    "isomorphic-fetch": "~2.2.1",
    "jest": "~28.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=4",
    "yarn": "<4.0.0"
  },
  "schematics": "./collection.json"
}
