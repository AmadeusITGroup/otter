    "@swc/helpers": "~0.5.0",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "~8.25.0",
    "jest-junit": "~16.0.0",
    "lint-staged": "^15.0.0",
    "minimist": "^1.2.6",
    "rimraf": "^6.0.1",
    "typedoc": "~0.27.0",
    "tsc-watch": "^6.0.4",
    "yaml-eslint-parser": "^1.2.2"
  },
  "openApiSupportedVersion": "^7.11.0",
  "engines": {
    "node": "^18.19.1 || ^20.11.1 || >=22.0.0"
  }
}
