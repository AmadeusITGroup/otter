    "mem-fs": "^4.0.0",
    "npm-run-all2": "^7.0.0",
    "nx": "~20.4.0",
    "openapi-types": "^12.0.0",
    "pid-from-port": "^1.1.3",
    "semver": "^7.5.2",
    "ts-jest": "~29.2.5",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.7.3",
    "typescript-eslint": "~8.25.0"
  },
  "generatorDependencies": {
    "@swc/cli": "~0.6.0",
