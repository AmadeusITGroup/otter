{
  "name": "@ama-sdk/swagger-builder",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Swagger API Builder",
  "readme": "./readme.md",
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-sdk-swagger-builder",
    "build:source": "tsc -b tsconfig.build.json --pretty && yarn cpy ./package.json src/schemas/*.json dist",
    "postbuild": "patch-package-json-main"
  },
  "bin": {
    "swagger-build": "./dist/src/cli/build.js",
    "swagger-dictionary-check": "./dist/src/cli/dictionary-check.js",
    "swagger-multi-success-check": "./dist/src/cli/multi-success-check.js",
    "swagger-operation-id-check": "./dist/src/cli/operation-id-check.js"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.0",
    "commander": "^13.0.0",
    "globby": "^11.1.0",
    "js-yaml": "^4.1.0",
    "jsonschema": "~1.5.0",
    "openapi-types": "^12.0.0",
    "semver": "^7.5.2",
    "swagger-schema-official": "~2.0.0-bab6bed",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~20.8.0",
    "@nx/jest": "~20.8.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@stylistic/eslint-plugin": "~3.1.0",
    "@types/glob": "^8.0.0",
    "@types/jest": "~29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.3.13",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/parser": "~8.33.0",
    "angular-eslint": "~19.4.0",
    "concurrently": "^9.1.0",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^5.0.0",
    "eslint": "~9.28.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~3.10.0",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~28.12.0",
    "eslint-plugin-jsdoc": "~50.6.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~56.0.0",
    "eslint-plugin-unused-imports": "~4.1.4",
    "globals": "^15.9.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "ts-jest": "~29.3.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "~8.33.0"
  },
  "engines": {
    "node": "^20.11.1 || >=22.0.0"
  }
}
