{
  "name": "@ama-sdk/swagger-builder",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Swagger API Builder",
  "readme": "./readme.md",
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-sdk-swagger-builder",
    "build:source": "tsc -b tsconfig.build.json --pretty && yarn cpy ./package.json src/schemas/*.json dist",
    "postbuild": "patch-package-json-main"
  },
  "bin": {
    "swagger-build": "./dist/src/cli/build.js",
    "swagger-dictionary-check": "./dist/src/cli/dictionary-check.js",
    "swagger-multi-success-check": "./dist/src/cli/multi-success-check.js",
    "swagger-operation-id-check": "./dist/src/cli/operation-id-check.js"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "commander": "^14.0.0",
    "globby": "^11.1.0",
    "js-yaml": "^4.1.1",
    "jsonschema": "~1.5.0",
    "openapi-types": "^12.0.0",
    "semver": "^7.5.2",
    "swagger-schema-official": "~2.0.0-bab6bed",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~22.3.3",
    "@nx/jest": "~22.3.3",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@stylistic/eslint-plugin": "~5.7.0",
    "@types/jest": "~30.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "~24.10.0",
    "@types/semver": "^7.3.13",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/parser": "~8.54.0",
    "angular-eslint": "~21.0.1",
    "concurrently": "^9.1.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.12.0",
    "eslint-plugin-jsdoc": "~61.7.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~62.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~30.2.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~30.2.0",
    "jsonc-eslint-parser": "~2.4.0",
    "ts-jest": "~29.4.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.54.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  }
}
