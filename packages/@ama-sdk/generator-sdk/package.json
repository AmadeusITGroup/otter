{
  "name": "@ama-sdk/generator-sdk",
  "version": "0.0.0-placeholder",
  "description": "Swagger specification SDK Generator",
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "test": "yarn nx test ama-sdk-generator-sdk",
    "build": "yarn nx build ama-sdk-generator-sdk",
    "postbuild": "patch-package-json-main",
    "build:swagger": "run-p 'build:swagger-*'",
    "build:swagger-typescript-gen": "mvn -s ./src/generators/core/templates/swagger-codegen-typescript/settings.xml clean package -f ./src/generators/core/templates/swagger-codegen-typescript/pom.xml",
    "build:swagger-dart-gen": "mvn -s ./src/generators/dart-core/templates/swagger-codegen-dart/settings.xml clean package -f ./src/generators/dart-core/templates/swagger-codegen-dart/pom.xml",
    "build:swagger-java-client-gen": "mvn -s ./src/generators/java-client-core/templates/swagger-codegen-java-client/settings.xml clean package -f ./src/generators/java-client-core/templates/swagger-codegen-java-client/pom.xml",
    "build:swagger-java-mdk-gen": "mvn -s ./src/generators/java-mdk-core/templates/swagger-codegen-mdk/settings.xml clean package -f ./src/generators/java-mdk-core/templates/swagger-codegen-mdk/pom.xml",
    "build:swagger-java-resteasy-gen": "mvn -s ./src/generators/java-resteasy-client-core/templates/swagger-codegen-java-resteasy-client/settings.xml clean package -f ./src/generators/java-resteasy-client-core/templates/swagger-codegen-java-resteasy-client/pom.xml",
    "build:swagger-java-swift-gen": "mvn -s ./src/generators/swift-core/templates/swagger-codegen-swift/settings.xml clean package -f ./src/generators/swift-core/templates/swagger-codegen-swift/pom.xml"
  },
  "schematics": "./dist/collection.json",
  "keywords": [
    "yeoman-generator"
  ],
  "repository": "https://github.com/AmadeusITGroup/otter",
  "bugs": "https://github.com/AmadeusITGroup/otter/issues",
