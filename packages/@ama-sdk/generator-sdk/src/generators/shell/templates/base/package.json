    "lint-staged": "^12.0.0",
    "minimist": "^1.2.5",
    "rimraf": "~3.0.2",
    "standard-version": "~9.5.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.23.0",
    "tsc-watch": "^5.0.0",
    "typescript": "~4.7.4",
    "yo": "~4.3.0"
  },
  "peerDependencies": {
    "@ama-sdk/core": "~<%= sdkCoreVersion %>",
    "isomorphic-fetch": "~2.2.1"
  },
