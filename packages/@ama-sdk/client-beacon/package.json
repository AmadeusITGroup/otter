{
  "name": "@ama-sdk/client-beacon",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Beacon API Request client for @ama-sdk/core based SDK",
  "module": "dist/src/public_api.js",
  "esm2015": "dist/esm2015/public_api.js",
  "esm2020": "dist/src/public_api.js",
  "types": "dist/src/public_api.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "module": "./dist/src/public_api.js",
      "types": "./dist/src/public_api.d.ts",
      "import": "./dist/src/public_api.js",
      "node": "./dist/cjs/public_api.js",
      "require": "./dist/cjs/public_api.js",
      "default": "./dist/cjs/public_api.js"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-sdk-client-beacon",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs -q --strip-leading-paths",
    "build:esm2015": "swc src -d dist/esm2015 -C module.type=es6 -q --strip-leading-paths",
    "build:esm2020": "tsc -b tsconfig.build.json",
    "postbuild": "yarn cpy './package.json' dist && patch-package-json-main",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "dependencies": {
    "@o3r/schematics": "workspace:^",
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.6.2",
    "uuid": "^11.0.5"
  },
  "peerDependencies": {
    "@ama-sdk/core": "workspace:^",
    "@angular-devkit/schematics": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "^20.0.0",
    "type-fest": "^4.30.1",
    "typescript": "^5.8.0"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@angular/cli": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@schematics/angular": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ama-sdk/core": "workspace:^",
    "@angular-devkit/core": "~20.0.0",
    "@angular-devkit/schematics": "~20.0.0",
    "@angular/common": "~20.0.0",
    "@angular/core": "~20.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~21.3.11",
    "@nx/jest": "~21.3.11",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@schematics/angular": "~20.0.0",
    "@stylistic/eslint-plugin": "~5.2.0",
    "@swc/cli": "~0.7.7",
    "@swc/core": "~1.13.0",
    "@types/jest": "~29.5.2",
    "@types/node": "^22.0.0",
    "@typescript-eslint/parser": "~8.41.0",
    "angular-eslint": "~20.1.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.34.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.0.0",
    "eslint-plugin-jsdoc": "~54.1.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~60.0.0",
    "eslint-plugin-unused-imports": "~4.2.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~29.7.0",
    "jsonc-eslint-parser": "~2.4.0",
    "minimist": "^1.2.6",
    "pid-from-port": "^1.1.3",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.4.0",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "~8.41.0",
    "zone.js": "~0.15.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  },
  "schematics": "./collection.json"
}
