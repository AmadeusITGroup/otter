{
  "name": "@ama-styling/figma-extractor",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Tools to extract Design Token from the Figma files",
  "keywords": [
    "figma",
    "design-token",
    "otter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-figma-extractor",
    "build:source": "tsc -b tsconfig.build.json --pretty && yarn cpy ./package.json dist",
    "prepare:build:builders": "yarn cpy 'collection.json' dist && yarn cpy 'schematics/**/*.json' dist/schematics",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "bin": "./cli/figma-extract.js",
  "generatorDependencies": {
    "@ama-styling/style-dictionary": "workspace:^",
    "style-dictionary": "~4.4.0"
  },
  "peerDependencies": {
    "@ama-sdk/client-fetch": "workspace:^",
    "@ama-sdk/core": "workspace:^",
    "@angular-devkit/schematics": "~19.2.0",
    "@o3r/schematics": "workspace:^",
    "type-fest": "^4.30.1"
  },
  "peerDependenciesMeta": {
    "@ama-sdk/client-fetch": {
      "optional": true
    },
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    }
  },
  "dependencies": {
    "@ama-sdk/client-fetch": "workspace:^",
    "@ama-sdk/core": "workspace:^",
    "@ama-styling/figma-sdk": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "semver": "^7.5.2",
    "tslib": "^2.6.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@ama-sdk/client-fetch": "workspace:^",
    "@ama-sdk/core": "workspace:^",
    "@angular-devkit/schematics": "~19.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint": "~20.8.0",
    "@nx/eslint-plugin": "~20.8.0",
    "@nx/jest": "~20.8.0",
    "@nx/js": "~20.8.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@stylistic/eslint-plugin": "~3.1.0",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.3.13",
    "@types/yargs": "~17.0.33",
    "@typescript-eslint/parser": "~8.39.0",
    "angular-eslint": "~19.8.0",
    "cpy-cli": "^5.0.0",
    "eslint": "~9.33.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~3.10.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~28.14.0",
    "eslint-plugin-jsdoc": "~50.8.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~56.0.0",
    "eslint-plugin-unused-imports": "~4.1.4",
    "globals": "^15.9.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jsonc-eslint-parser": "~2.4.0",
