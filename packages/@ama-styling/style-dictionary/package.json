{
  "name": "@ama-styling/style-dictionary",
  "version": "0.0.0-placeholder",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/src/public_api.d.mts",
  "main": "./dist/src/public_api.mjs",
  "description": "Package exposing modules and hooks for Style Dictionary",
  "keywords": [
    "design",
    "otter",
    "otter-module",
    "style-dictionary"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "copy:schemas": "yarn cpy 'schemas/**/*' dist/schemas",
    "copy:templates": "yarn cpy 'schematics/**/templates/**' dist/schematics",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "build:source": "tsc -b tsconfig.build.json && yarn cpy package.json dist/",
    "build": "yarn nx build design",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist && yarn copy:templates",
    "postbuild": "patch-package-json-main"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    "./schemas/*.json": {
      "default": "./schemas/*.json"
    },
    ".": {
      "module": "./dist/src/public_api.mjs",
      "types": "./dist/src/public_api.d.mts",
      "default": "./dist/src/public_api.mjs"
    }
  },
  "dependencies": {
    "@angular-devkit/architect": "~0.2101.0",
    "@angular-devkit/schematics": "~21.1.0",
    "@o3r/schematics": "workspace:~",
    "@schematics/angular": "~21.1.0",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@o3r/core": "workspace:~",
    "@o3r/schematics": "workspace:~",
    "@o3r/telemetry": "workspace:~",
    "style-dictionary": "^5.0.0",
    "type-fest": "^5.3.1"
  },
  "peerDependenciesMeta": {
    "@o3r/core": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@o3r/telemetry": {
      "optional": true
    },
    "chokidar": {
      "optional": true
    },
    "globby": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular/cli": "~21.1.0",
    "@babel/core": "~7.29.0",
    "@babel/preset-env": "~7.29.0",
    "@babel/preset-typescript": "~7.28.0",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint": "~22.5.0",
    "@nx/eslint-plugin": "~22.5.0",
    "@nx/jest": "~22.5.0",
    "@nx/js": "~22.5.0",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/core": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/telemetry": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@schematics/angular": "~21.1.0",
    "@stylistic/eslint-plugin": "~5.7.0",
    "@types/jest": "~30.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "~24.10.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/parser": "~8.54.0",
    "angular-eslint": "~21.2.0",
    "babel-jest": "^30.0.5",
    "chokidar": "~5.0.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.12.0",
    "eslint-plugin-jsdoc": "~61.7.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~62.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~30.2.0",
    "jest-junit": "~16.0.0",
    "jest-resolve": "~30.2.0",
    "jest-util": "~30.2.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~22.5.0",
    "style-dictionary": "~5.2.0",
    "ts-jest": "~29.4.0",
    "type-fest": "^5.3.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.54.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  },
  "schematics": "./collection.json"
}
