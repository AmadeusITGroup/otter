    "typescript": "~5.2.2"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/architect": {
      "optional": true
    },
    "@angular-devkit/core": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^4.1.2",
