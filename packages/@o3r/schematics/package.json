    "@babel/core": "~7.23.0",
    "@babel/preset-typescript": "~7.23.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nx/eslint": "~17.1.1",
    "@nx/eslint-plugin": "~17.1.1",
    "@nx/jest": "~17.1.1",
