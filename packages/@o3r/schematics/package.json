{
  "name": "@o3r/schematics",
  "version": "0.0.0-placeholder",
  "description": "Schematics module of the Otter framework",
  "main": "./dist/src/index.js",
  "typings": "./src/public_api.d.ts",
  "keywords": [
    "schematics",
    "otter"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build schematics",
