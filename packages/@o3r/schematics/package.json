{
  "name": "@o3r/schematics",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Schematics module of the Otter framework",
  "main": "./src/public_api.ts",
  "typings": "./src/public_api.ts",
  "keywords": [
    "schematics",
    "otter"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build schematics",
    "postbuild": "yarn cpy 'package.json' dist && patch-package-json-main",
    "prepare:publish": "prepare-publish ./dist",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:source": "tsc -b tsconfig.build.json --pretty",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "build:cli": "tsc -b tsconfig.cli.json --pretty && yarn generate-cjs-manifest"
  },
  "bin": {
    "o3r-environment": "./dist/cli/environment.js"
  },
  "peerDependencies": {
    "@angular-devkit/architect": "~0.1802.0",
    "@angular-devkit/core": "~18.2.0",
    "@angular-devkit/schematics": "~18.2.0",
    "@angular/cli": "~18.2.0",
    "@o3r/telemetry": "workspace:^",
    "@schematics/angular": "~18.2.0",
    "eslint": "^8.57.0",
    "rxjs": "^7.8.1",
    "type-fest": "^4.10.2",
    "typescript": "~5.5.4"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/architect": {
      "optional": true
    },
    "@angular-devkit/core": {
      "optional": true
    },
    "@angular/cli": {
      "optional": true
    },
    "@o3r/telemetry": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "globby": "^11.1.0",
    "inquirer": "~8.2.6",
    "minimatch": "~9.0.3",
    "semver": "^7.5.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.0",
    "@angular-devkit/core": "~18.2.0",
    "@angular-devkit/schematics": "~18.2.0",
    "@angular-eslint/eslint-plugin": "~18.3.0",
    "@angular/animations": "~18.2.0",
    "@angular/cli": "~18.2.0",
    "@angular/common": "~18.2.0",
    "@angular/compiler": "~18.2.0",
    "@angular/compiler-cli": "~18.2.0",
    "@angular/core": "~18.2.0",
    "@angular/platform-browser": "~18.2.0",
    "@angular/platform-browser-dynamic": "~18.2.0",
    "@babel/core": "~7.26.0",
    "@babel/preset-typescript": "~7.26.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nx/eslint": "~19.5.0",
    "@nx/eslint-plugin": "~19.5.0",
    "@nx/jest": "~19.5.0",
    "@nx/js": "~19.5.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "@schematics/angular": "~18.2.0",
    "@stylistic/eslint-plugin-ts": "~2.4.0",
    "@types/inquirer": "~8.2.10",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
