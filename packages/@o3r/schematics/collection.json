{
  "$schema": "https://raw.githubusercontent.com/angular/angular-cli/main/packages/angular_devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Add Otter schematics to the project.",
      "factory": "./schematics/ng-add/index#ngAdd",
      "schema": "./schematics/ng-add/schema.json",
      "aliases": ["install", "i"]
    },
    "eslint": {
      "description": "Eslint Otter schematics",
      "factory": "./schematics/eslint/index#eslint",
      "schema": "./schematics/eslint/schema.json",
      "aliases": ["lint"],
      "hidden": true
    }
  }
}
