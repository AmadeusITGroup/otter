{
  "$schema": "https://raw.githubusercontent.com/ng-packagr/ng-packagr/master/src/ng-package.schema.json",
  "dest": "./dist",
  "deleteDestPath": false,
  "allowedNonPeerDependencies": [
    "@o3r/schematics"
  ],
  "lib": {
    "entryFile": "src/public_api.ts"
  },
  "assets": [
    "schemas/*.json",
    "scss/**/*",
    "!scss/**/*.spec.ts",
    "assets/**",
    "_index.scss",
    "_otter-theme.scss"
  ]
}
