{
  "name": "@o3r/styling",
  "version": "0.0.0-placeholder",
  "description": "A styling framework to apply a theme on an Otter application at build time and runtime to support CMS customization.",
  "keywords": [
    "styling",
    "otter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "test": "yarn nx test styling",
    "copy:templates": "yarn cpy 'schematics/**/templates/**/*.scss' dist/schematics",
    "prepare:build:builders": "yarn cpy 'builders/**/*.json' dist/builders && yarn cpy '{builders,collection}.json' dist && yarn cpy 'schematics/**/*.json' dist/schematics && yarn copy:templates",
    "prepare:publish": "prepare-publish ./dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "build": "yarn nx build styling"
  },
  "exports": {
    ".": {
      "sass": "./_index.scss"
    },
    "./otter-theme": {
      "sass": "./theming/otter-theme/_index.scss"
    }
  },
  "peerDependencies": {
    "@angular-devkit/architect": "~0.1602.0",
    "@angular-devkit/core": "~16.2.0",
    "@angular-devkit/schematics": "~16.2.0",
    "@angular/core": "~16.2.0",
    "@angular/material": "~16.2.0",
    "@o3r/core": "workspace:^",
    "@o3r/dynamic-content": "workspace:^",
    "@o3r/extractors": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "~16.2.0",
    "rxjs": "^7.8.1",
    "sass": "~1.69.0"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@o3r/extractors": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@schematics/angular": {
      "optional": true
    },
    "sass": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1602.0",
    "@angular-devkit/build-angular": "~16.2.0",
    "@angular-devkit/core": "~16.2.0",
    "@angular-devkit/schematics": "~16.2.0",
    "@angular-eslint/eslint-plugin": "~16.2.0",
    "@angular/animations": "~16.2.0",
    "@angular/cdk": "~16.2.0",
    "@angular/common": "~16.2.0",
    "@angular/compiler": "~16.2.0",
    "@angular/compiler-cli": "~16.2.0",
    "@angular/core": "~16.2.0",
    "@angular/forms": "~16.2.0",
    "@angular/material": "~16.2.0",
    "@angular/platform-browser": "~16.2.0",
    "@angular/platform-browser-dynamic": "~16.2.0",
    "@babel/core": "~7.23.0",
    "@babel/preset-typescript": "~7.23.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nx/eslint-plugin": "~16.8.0",
    "@nx/jest": "~16.8.0",
    "@nx/js": "~16.8.0",
    "@nx/linter": "~16.8.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "@o3r/dynamic-content": "workspace:^",
    "@o3r/eslint-config-otter": "workspace:^",
