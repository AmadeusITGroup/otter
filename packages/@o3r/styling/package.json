    "@yarnpkg/core": "^4.1.1",
    "@yarnpkg/fslib": "^3.1.0",
    "@yarnpkg/plugin-npm": "^3.0.1",
    "@yarnpkg/plugin-pack": "^4.0.0",
    "chokidar": "^4.0.3",
    "globby": "^11.1.0",
    "rxjs": "^7.8.1",
    "sass": "^1.81.0",
