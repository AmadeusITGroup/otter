    "eslint-plugin-jest": "~27.4.0",
    "eslint-plugin-jsdoc": "~46.8.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "globby": "^11.1.0",
    "jest": "~29.7.0",
