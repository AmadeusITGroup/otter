    "ts-jest": "^28.0.5",
    "typescript": "~4.9.4",
    "zone.js": "^0.13.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "builders": "./builders.json",
  "schematics": "./collection.json"
}
