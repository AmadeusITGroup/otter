    "jest-junit": "~16.0.0",
    "jest-preset-angular": "~14.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "logrocket": "~8.1.0",
    "logrocket-ngrx": "~0.2.1",
    "ng-packagr": "~17.2.0",
    "nx": "~18.0.2",
    "rxjs": "^7.8.1",
    "smartlook-client": "^8.0.0",
    "ts-jest": "~29.1.1",
    "ts-node": "~10.9.1",
    "typescript": "~5.3.3",
    "zone.js": "~0.14.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "schematics": "./collection.json"
}
