{
  "name": "@o3r/logger",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "This module provides a bridge to target different loggers (logrocket, fullstory, smartlook) with a common interface.\nIt is reporting the logs emitted by the different Otter Modules.",
  "keywords": [
    "logger",
    "otter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build logger",
    "prepare:compile": "cp-package-json",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "prepare:publish": "prepare-publish ./dist"
  },
  "peerDependencies": {
    "@angular/core": "~17.2.0",
    "@angular/platform-browser-dynamic": "~17.2.0",
    "@fullstory/browser": "^2.0.0",
    "@ngrx/store": "~17.1.0",
    "@o3r/core": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "logrocket": "~8.1.0",
    "logrocket-ngrx": "~0.2.1",
    "rxjs": "^7.8.1",
    "smartlook-client": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@fullstory/browser": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "logrocket": {
      "optional": true
    },
    "logrocket-ngrx": {
      "optional": true
    },
    "smartlook-client": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.2.0",
    "@angular-devkit/core": "~17.2.0",
    "@angular-devkit/schematics": "~17.2.0",
    "@angular-eslint/eslint-plugin": "~17.2.0",
    "@angular/animations": "~17.2.0",
    "@angular/common": "~17.2.0",
    "@angular/compiler": "~17.2.0",
    "@angular/compiler-cli": "~17.2.0",
    "@angular/core": "~17.2.0",
    "@angular/platform-browser": "~17.2.0",
    "@angular/platform-browser-dynamic": "~17.2.0",
    "@babel/core": "~7.24.0",
    "@babel/preset-typescript": "~7.23.0",
    "@compodoc/compodoc": "^1.1.19",
    "@fullstory/browser": "^2.0.0",
    "@ngrx/store": "~17.1.0",
    "@nx/eslint": "~18.0.2",
    "@nx/eslint-plugin": "~18.0.2",
    "@nx/jest": "~18.0.2",
    "@nx/js": "~18.0.2",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/eslint-config-otter": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@stylistic/eslint-plugin-ts": "^1.5.4",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "~27.9.0",
    "eslint-plugin-jsdoc": "~48.2.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^51.0.0",
