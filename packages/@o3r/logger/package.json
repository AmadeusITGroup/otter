{
  "name": "@o3r/logger",
  "version": "0.0.0-placeholder",
  "description": "This module provides a bridge to target different loggers (logrocket, fullstory, smartlook) with a common interface.\nIt is reporting the logs emitted by the different Otter Modules.",
  "keywords": [
    "logger",
    "otter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build logger",
    "prepare:compile": "cp-package-json",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty",
    "prepare:publish": "prepare-publish ./dist"
  },
  "peerDependencies": {
    "@angular/core": "~16.1.0",
    "@angular/platform-browser-dynamic": "~16.1.0",
    "@fullstory/browser": "^1.2.1",
    "@ngrx/store": "~16.0.1",
    "@o3r/core": "workspace:^",
    "logrocket": "~4.0.0",
    "logrocket-ngrx": "~0.2.1",
    "rxjs": "^7.8.1",
    "smartlook-client": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@fullstory/browser": {
      "optional": true
    },
    "logrocket": {
      "optional": true
    },
    "logrocket-ngrx": {
      "optional": true
    },
    "smartlook-client": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.1.0",
    "@angular-devkit/core": "~16.1.0",
    "@angular-devkit/schematics": "~16.1.0",
    "@angular-eslint/eslint-plugin": "~16.0.3",
    "@angular/common": "~16.1.0",
    "@angular/compiler": "~16.1.0",
    "@angular/compiler-cli": "~16.1.0",
    "@angular/core": "~16.1.0",
    "@angular/platform-browser": "~16.1.0",
    "@angular/platform-browser-dynamic": "~16.1.0",
    "@babel/core": "~7.22.0",
    "@babel/preset-typescript": "~7.22.0",
    "@compodoc/compodoc": "^1.1.19",
    "@fullstory/browser": "~1.7.0",
    "@ngrx/store": "~16.0.1",
    "@nx/jest": "~16.3.2",
    "@nx/js": "~16.3.2",
    "@nx/linter": "~16.3.2",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "@o3r/eslint-config-otter": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@types/jest": "~29.5.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "jest": "~29.5.0",
    "jest-environment-jsdom": "~29.5.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^13.1.1",
    "logrocket": "~4.0.0",
    "logrocket-ngrx": "~0.2.1",
    "ng-packagr": "~16.1.0",
    "nx": "~16.3.2",
    "rxjs": "^7.8.1",
    "smartlook-client": "^8.0.0",
    "typescript": "~5.0.2",
    "zone.js": "~0.13.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "schematics": "./collection.json"
}
