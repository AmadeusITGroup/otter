  },
  "cmsMetadata": {
    "localizationFilePath": "./localisation.metadata.json",
    "componentFilePath": "./component.class.metadata.json",
    "configurationFilePath": "./component.config.metadata.json",
    "styleFilePath": "./style.metadata.json",
