    "@ngrx/store": "~18.0.0",
    "@ngx-translate/core": "~15.0.0",
    "@nx/eslint": "~19.5.0",
    "@nx/eslint-plugin": "~19.5.0",
    "@nx/jest": "~19.5.0",
    "@nx/js": "~19.5.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/eslint-config-otter": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
