{
  "name": "@o3r/eslint-config-otter",
  "version": "0.0.0-placeholder",
  "description": "Recommended eslint configuration for Otter project",
  "main": "src/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "default": "./index.js"
    },
    "./base": {
      "default": "./base.js"
    },
    "./fast": {
      "default": "./fast.js"
    },
    "./jasmine": {
      "default": "./jasmine.js"
    },
    "./jasmine-fast": {
      "default": "./jasmine-fast.js"
    },
    "./template": {
      "default": "./template.js"
    }
  },
  "keywords": [
    "eslint",
    "otter",
    "amadeus",
    "linter",
    "otter-module"
  ],
  "scripts": {
    "build": "yarn cpy --flat {base,fast,index,jasmine,jasmine-fast,template}.js package.json dist && yarn postbuild",
    "postbuild": "patch-package-json-main",
    "prepare:publish": "prepare-publish ./dist",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "~16.1.6",
    "@angular-eslint/builder": "~16.1.0",
    "@angular-eslint/eslint-plugin": "~16.1.0",
    "@angular-eslint/eslint-plugin-template": "~16.1.0",
    "@angular-eslint/template-parser": "~16.1.0",
    "@angular/compiler": "~16.1.6",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "~16.1.6",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.22.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unicorn": "^47.0.0",
    "typescript": "~5.1.6"
  },
  "devDependencies": {
    "@angular-devkit/core": "~16.1.6",
    "@angular-devkit/schematics": "~16.1.6",
    "@angular-eslint/eslint-plugin": "~16.1.0",
    "@angular-eslint/eslint-plugin-template": "~16.1.0",
    "@angular-eslint/template-parser": "~16.1.0",
    "@angular/compiler": "~16.1.6",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "~16.1.6",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jest": "~27.2.3",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "nx": "~16.6.0",
    "typescript": "~5.1.6"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@angular-eslint/builder": {
      "optional": true
    },
    "@angular-eslint/eslint-plugin-template": {
      "optional": true
    },
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "@angular/compiler": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@schematics/angular": {
      "optional": true
    },
    "eslint-plugin-jasmine": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "schematics": "./collection.json"
}
