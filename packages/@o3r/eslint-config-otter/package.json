{
  "name": "@o3r/eslint-config-otter",
  "version": "0.0.0-placeholder",
  "description": "TSLint rules",
  "main": "index.js",
  "keywords": [
    "eslint",
    "otter",
    "amadeus",
    "linter"
  ],
  "scripts": {
    "build": "yarn cpy '{index,template,fast}.js' package.json dist && yarn postbuild",
    "postbuild": "patch-package-json-main",
    "prepare:publish": "prepare-publish ./dist",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "~15.1.3",
    "@angular-eslint/eslint-plugin": "~15.2.0",
    "@angular-eslint/eslint-plugin-template": "~15.2.0",
    "@angular-eslint/template-parser": "~15.2.0",
    "@angular/compiler": "~15.1.2",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.22.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@angular-devkit/core": "~15.1.3",
    "@angular-devkit/schematics": "~15.1.3",
    "@angular-eslint/eslint-plugin": "~15.2.0",
    "@angular-eslint/eslint-plugin-template": "~15.2.0",
    "@angular-eslint/template-parser": "~15.2.0",
    "@angular/compiler": "~15.1.2",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.22.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "nx": "^15.6.3",
    "typescript": "~4.9.4"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@angular-eslint/eslint-plugin-template": {
      "optional": true
    },
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "@angular/compiler": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "schematics": "./collection.json"
}
