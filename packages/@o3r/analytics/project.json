{
  "name": "analytics",
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/@o3r/analytics/src",
  "prefix": "o3r",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/dist/package.json"
      ],
      "options": {
        "script": "postbuild"
      },
      "dependsOn": [
        "build-builders",
        "compile",
        "build-plugins",
        "build-fixtures"
      ]
    },
    "compile": {
      "executor": "@angular-devkit/build-angular:ng-packagr",
      "dependsOn": [
        "prepare",
        "^build"
      ],
      "options": {
        "project": "packages/@o3r/analytics/src/ng-package.json",
        "tsConfig": "packages/@o3r/analytics/tsconfig.build.json"
      }
    },
    "prepare": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/src/package.json"
      ],
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "{projectRoot}/package.json"
      ],
      "options": {
        "script": "prepare:compile"
      }
    },
    "build-fixtures": {
      "executor": "nx:noop"
    },
    "build-fixtures-jest": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:fixtures:jest"
      }
    },
    "build-fixtures-jasmine": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:fixtures:jasmine"
      }
    },
    "build-plugins": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:plugins"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "configurations": {
        "ci": {
          "quiet": true,
          "cacheLocation": ".cache/eslint"
        }
      },
      "options": {
        "eslintConfig": "packages/@o3r/analytics/.eslintrc.js",
        "lintFilePatterns": [
          "packages/@o3r/analytics/src/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/@o3r/analytics/jest.config.js",
        "silent": true
      }
    },
    "prepare-publish": {
      "executor": "nx:run-script",
      "options": {
        "script": "prepare:publish"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish packages/@o3r/analytics/dist"
      }
    },
    "prepare-build-builders": {
      "executor": "nx:run-script",
      "options": {
        "script": "prepare:build:builders"
      }
    },
    "build-builders": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:builders"
      }
    },
    "documentation": {
      "executor": "nx:run-script",
      "options": {
        "script": "compodoc"
      }
    }
  },
  "tags": []
}
