{
  "name": "@o3r/analytics",
  "version": "0.0.0-placeholder",
  "description": "This module provides a framework to capture, store and report multiple events (ui, performance etc.) emitted by an Otter Application.",
  "keywords": [
    "analytics",
    "otter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build:plugins": "tsc -b tsconfig.plugins.json",
    "postbuild": "patch-package-json-exports",
    "build": "yarn nx build analytics",
    "prepare:compile": "cp-package-json",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty",
    "build:fixtures:jest": "tsc -b tsconfig.fixture.jest.json --pretty",
    "build:fixtures:jasmine": "tsc -b tsconfig.fixture.jasmine.json --pretty",
    "prepare:publish": "prepare-publish ./dist"
  },
  "peerDependencies": {
    "@ama-sdk/core": "workspace:^",
    "@angular/common": "^15.1.2",
    "@angular/core": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/router": "^15.1.2",
    "@ngrx/store": "^15.0.0",
    "@o3r/core": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "jasmine": "^4.0.0",
    "rxjs": "^7.4.0",
    "webpack": "~5.84.0"
  },
  "peerDependenciesMeta": {
    "jasmine": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@ama-sdk/core": "workspace:^",
    "@angular-devkit/build-angular": "~15.2.0",
    "@angular-devkit/core": "~15.2.0",
    "@angular-devkit/schematics": "~15.2.0",
    "@angular-eslint/eslint-plugin": "~15.2.0",
    "@angular/common": "~15.2.0",
    "@angular/compiler": "~15.2.0",
    "@angular/compiler-cli": "~15.2.0",
    "@angular/core": "~15.2.0",
    "@angular/platform-browser": "~15.2.0",
    "@angular/platform-browser-dynamic": "~15.2.0",
    "@angular/router": "~15.2.0",
    "@babel/core": "~7.21.0",
    "@babel/preset-typescript": "~7.21.0",
    "@compodoc/compodoc": "^1.1.19",
    "@ngrx/store": "~15.4.0",
    "@nrwl/cli": "~15.9.0",
    "@nrwl/jest": "~15.9.0",
    "@nrwl/js": "~15.9.0",
    "@nrwl/linter": "~15.9.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@types/jasmine": "3.10.7",
    "@types/jest": "~28.1.2",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.22.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^45.0.2",
    "jest": "~28.1.1",
    "jest-environment-jsdom": "~28.1.1",
    "jest-junit": "^14.0.0",
    "jest-preset-angular": "^12.2.3",
    "nx": "~15.9.0",
    "rxjs": "^7.4.0",
    "type-fest": "^3.6.1",
    "typescript": "~4.9.4",
    "webpack": "~5.84.0",
    "zone.js": "^0.13.0"
  },
  "exports": {
    "./plugins/webpack": {
      "types": "./plugins/webpack/index.d.ts",
      "default": "./plugins/webpack/index.js"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "builders": "./builders.json",
  "schematics": "./collection.json"
}
