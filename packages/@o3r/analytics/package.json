{
  "name": "@o3r/analytics",
  "version": "0.0.0-placeholder",
  "description": "analytics module of the Otter framework",
  "keywords": [
    "analytics",
    "otter"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build:plugins": "tsc -b tsconfig.plugins.json",
    "postbuild": "patch-package-json-exports",
    "build": "yarn nx build analytics",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty",
    "prepare:publish": "dispatch-package-json-fields ./dist/package.json"
  },
  "peerDependencies": {
    "@ama-sdk/core": "workspace:^",
    "@angular/common": "^15.0.2",
    "@angular/core": "^15.0.2",
    "@angular/platform-browser": "^15.0.2",
    "@angular/platform-browser-dynamic": "^15.0.2",
    "@angular/router": "^15.0.2",
    "@ngrx/store": "^15.0.0",
    "@o3r/core": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "rxjs": "^7.4.0",
    "webpack": "~5.75.0"
  },
  "peerDependenciesMeta": {
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@ama-sdk/core": "workspace:^",
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular-devkit/core": "~15.0.4",
    "@angular-devkit/schematics": "~15.0.4",
    "@angular/common": "~15.0.4",
    "@angular/compiler": "~15.0.4",
    "@angular/compiler-cli": "~15.0.4",
    "@angular/core": "~15.0.4",
    "@angular/platform-browser": "~15.0.4",
    "@angular/platform-browser-dynamic": "~15.0.4",
    "@angular/router": "~15.0.4",
    "@ngrx/store": "^15.0.0",
    "@nrwl/cli": "^15.3.3",
    "@nrwl/js": "^15.3.3",
    "@nrwl/linter": "^15.3.3",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "@o3r/logger": "workspace:^",
    "@types/jest": "~28.1.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "~28.1.1",
    "jest-junit": "^14.0.0",
    "jest-preset-angular": "^12.2.3",
    "nx": "^15.3.3",
    "rxjs": "^7.4.0",
    "type-fest": "^2.13.1",
    "typescript": "~4.8.4",
    "webpack": "~5.75.0",
    "zone.js": "^0.12.0"
  },
  "exports": {
    "./plugins/webpack": {
      "types": "./plugins/webpack/index.d.ts",
      "default": "./plugins/webpack/index.js"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "builders": "./builders.json",
  "schematics": "./collection.json"
}
