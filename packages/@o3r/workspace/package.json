{
  "name": "@o3r/workspace",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Workspace tooling of the Otter Framework",
  "keywords": [
    "workspace",
    "otter"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/public_api.d.ts",
      "default": "./dist/src/public_api.js"
    },
    "./package.json": {
      "node": "./package.json"
    },
    "./builders/*": {
      "types": "./dist/builders/*.d.ts",
      "default": "./dist/builders/*"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build workspace",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "build:source": "tsc -b tsconfig.build.json --pretty && yarn cpy ./package.json dist",
    "prepare:build:builders": "yarn cpy 'builders/**/*.json' dist/builders && yarn cpy 'schematics/**/*.json' 'schematics/**/templates/**' dist/schematics && yarn cpy '{builders,collection,migration}.json' dist",
    "postbuild": "patch-package-json-main"
  },
  "main": "./dist/src/public_api.js",
  "types": "./dist/src/public_api.d.ts",
  "peerDependencies": {
    "@angular-devkit/architect": ">=0.1900.0 <0.2000.0-0",
    "@angular-devkit/core": "^19.0.0",
    "@angular-devkit/schematics": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@o3r/schematics": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "@schematics/angular": "^19.0.0",
    "ts-node": "~10.9.2",
    "type-fest": "^4.10.2",
    "typescript": "^5.5.4"
  },
  "peerDependenciesMeta": {
    "@angular/cli": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@o3r/telemetry": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    }
  },
  "dependencies": {
    "@o3r/schematics": "workspace:^",
    "commander": "^13.0.0",
    "globby": "^11.1.0",
    "semver": "^7.5.2",
    "tslib": "^2.6.2",
    "uuid": "^11.0.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1902.0",
    "@angular-devkit/build-angular": "~19.2.0",
    "@angular-devkit/core": "~19.2.0",
    "@angular-devkit/schematics": "~19.2.0",
    "@angular/animations": "~19.2.0",
    "@angular/cli": "~19.2.0",
    "@angular/common": "~19.2.0",
    "@angular/compiler": "~19.2.0",
    "@angular/compiler-cli": "~19.2.0",
    "@angular/core": "~19.2.0",
    "@angular/platform-browser": "~19.2.0",
    "@angular/platform-browser-dynamic": "~19.2.0",
    "@angular/router": "~19.2.0",
    "@babel/core": "~7.27.0",
    "@babel/preset-typescript": "~7.27.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@ngrx/entity": "~19.2.0",
    "@ngrx/store": "~19.2.0",
    "@nx/angular": "~20.8.0",
    "@nx/eslint": "~20.8.0",
    "@nx/eslint-plugin": "~20.8.0",
    "@nx/jest": "~20.8.0",
    "@nx/js": "~20.8.0",
    "@nx/workspace": "~20.8.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@schematics/angular": "~19.2.0",
    "@stylistic/eslint-plugin": "~3.1.0",
    "@types/jest": "~29.5.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.0.0",
    "@types/pid-from-port": "^1.1.0",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "~8.33.0",
    "angular-eslint": "~19.4.0",
    "cpy-cli": "^5.0.0",
    "eslint": "~9.28.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~3.10.0",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~28.12.0",
    "eslint-plugin-jsdoc": "~50.6.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~56.0.0",
    "eslint-plugin-unused-imports": "~4.1.4",
    "globals": "^15.9.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-preset-angular": "~14.5.0",
    "jsonc-eslint-parser": "~2.4.0",
    "minimist": "^1.2.6",
    "nx": "~20.8.0",
    "pid-from-port": "^1.1.3",
    "rxjs": "^7.8.1",
    "ts-jest": "~29.3.0",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "~8.33.0",
    "zone.js": "~0.15.0"
  },
  "generatorDependencies": {
    "@angular-builders/jest": "~19.0.0",
    "@angular/material": "~19.2.0",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-angular": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@commitlint/types": "^19.0.0",
    "@ngrx/router-store": "~19.2.0",
    "@ngrx/effects": "~19.2.0",
    "@ngrx/store-devtools": "~19.2.0",
    "editorconfig-checker": "^6.0.1",
    "husky": "~9.1.0",
    "lerna": "^8.1.7",
    "lint-staged": "^15.0.0"
  },
  "engines": {
    "node": "^20.11.1 || >=22.0.0"
  },
  "schematics": "./collection.json",
  "builders": "./builders.json",
  "ng-update": {
    "migrations": "./migration.json"
  },
  "bin": {
    "o3r-set-version": "./dist/src/cli/set-version.cjs"
  }
}
