    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@ngrx/entity": "~20.0.0",
    "@ngrx/store": "~20.0.0",
    "@nx/angular": "~21.5.0",
    "@nx/eslint": "~21.5.0",
