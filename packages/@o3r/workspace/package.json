{
  "name": "@o3r/workspace",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Workspace tooling of the Otter Framework",
  "keywords": [
    "workspace",
    "otter"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
