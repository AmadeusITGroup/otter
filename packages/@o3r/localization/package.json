    "@o3r/dynamic-content": "workspace:^",
    "@o3r/extractors": "workspace:^",
    "@o3r/logger": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "^20.0.0",
    "@yarnpkg/cli": "^4.3.1",
    "@yarnpkg/core": "^4.1.1",
    "@yarnpkg/fslib": "^3.1.0",
    "@yarnpkg/plugin-npm": "^3.0.1",
