{
  "name": "@o3r/localization",
  "version": "0.0.0-placeholder",
  "description": "Localization module of the Otter framework",
  "keywords": [
    "localization",
    "otter"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "copy:templates": "yarn cpy 'schematics/**/templates/**' dist/schematics",
    "prepare:build:builders": "yarn cpy 'builders/**/*.json' dist/builders && yarn cpy '{builders,collection}.json' dist && yarn cpy 'schematics/**/*.json' dist/schematics && yarn copy:templates",
    "prepare:publish": "dispatch-package-json-fields ./dist/package.json",
    "build:builders": "tsc -b tsconfig.builders.json --pretty",
    "build": "yarn nx build localization"
  },
  "peerDependencies": {
    "@angular-devkit/architect": "^0.1500.2",
    "@angular-devkit/core": "~15.0.4",
    "@angular/cdk": "~15.0.4",
    "@angular/common": "~15.0.4",
    "@angular/core": "~15.0.4",
    "@angular/platform-browser-dynamic": "~15.0.4",
    "@ngrx/store": "^15.0.0",
    "@ngx-translate/core": "~14.0.0",
    "@o3r/core": "workspace:^",
    "@o3r/dynamic-content": "workspace:^",
    "@o3r/extractors": "workspace:^",
    "@o3r/logger": "workspace:^",
    "@schematics/angular": "~15.0.4",
    "intl-messageformat": "~10.2.0",
    "rxjs": "^7.4.0"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/core": {
      "optional": true
    },
    "@angular-eslint/template-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@o3r/dev-tools": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1500.4",
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular-devkit/core": "~15.0.4",
    "@angular-devkit/schematics": "~15.0.4",
    "@angular/cdk": "~15.0.4",
    "@angular/common": "~15.0.4",
    "@angular/compiler": "~15.0.4",
    "@angular/compiler-cli": "~15.0.4",
    "@angular/core": "~15.0.4",
    "@angular/platform-browser": "~15.0.4",
    "@angular/platform-browser-dynamic": "~15.0.4",
    "@ngrx/store": "^15.0.0",
    "@ngx-translate/core": "~14.0.0",
    "@nrwl/cli": "^15.3.3",
    "@nrwl/js": "^15.3.3",
    "@nrwl/linter": "^15.3.3",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/dynamic-content": "workspace:^",
    "@o3r/eslint-config-otter": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/extractors": "workspace:^",
    "@o3r/logger": "workspace:^",
    "@schematics/angular": "~15.0.4",
    "@types/jest": "~28.1.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chokidar": "^3.5.2",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.22.0",
