  "scripts": {
    "build": "yarn nx build chrome-devtools",
    "postbuild:patch": "yarn patch:manifest && yarn patch:extension && yarn patch:package",
    "patch:package": "cpy 'package.json' 'dist' && patch-package-json-main",
    "patch:manifest": "node scripts/sanitize-manifest.cjs",
    "build:extension": "tsc -b tsconfig.extension.json",
    "patch:extension": "node scripts/sanitize-extension.cjs",
    "start": "concurrently 'yarn:watch:*'",
    "watch:panel": "yarn build:panel --watch",
    "watch:components": "yarn build:components --watch",
    "watch:manifest": "yarn patch:manifest --watch",
    "watch:extension": "tsc-watch -b tsconfig.extension.json --noClear --onSuccess 'yarn patch:extension'"
  },
  "files": [
    "dist/"
  ],
