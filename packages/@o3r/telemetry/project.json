{
  "name": "telemetry",
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/@o3r/telemetry/src",
  "prefix": "o3r",
  "implicitDependencies": [
    "!core"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/dist/package.json"
      ],
      "options": {
        "script": "postbuild",
        "tsConfig": "{projectRoot}/tsconfig.build.json"
      },
      "dependsOn": [
        "build-builders",
        "compile"
      ]
    },
    "compile": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:source"
      }
    },
    "prepare-build-builders": {},
    "build-builders": {},
    "lint": {},
    "test": {},
    "prepare-publish": {},
    "publish": {}
  },
  "tags": []
}
