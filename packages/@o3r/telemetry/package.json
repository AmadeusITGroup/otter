{
  "name": "@o3r/telemetry",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "A set of helpers to retrieve tool usage metrics.",
  "keywords": [
    "otter-module"
  ],
  "scripts": {
    "build": "yarn nx build telemetry",
    "build:builders": "tsc -b tsconfig.builders.json --pretty",
    "build:source": "tsc -b tsconfig.build.json --pretty && yarn cpy ./package.json dist",
    "postbuild": "patch-package-json-main",
    "prepare:build:builders": "yarn run cpy '{collection,migration}.json' dist && yarn run cpy 'schematics/**/*.json' dist/schematics"
  },
  "dependencies": {
    "minimist": "^1.2.6",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@angular-devkit/architect": ">=0.2000.0 <0.2100.0-0",
    "@angular-devkit/core": "^20.0.0",
    "@angular-devkit/schematics": "^20.0.0",
    "@o3r/core": "workspace:~",
    "rxjs": "^7.8.1"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/architect": {
      "optional": true
    },
    "@angular-devkit/core": {
      "optional": true
    },
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@o3r/core": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2003.13",
    "@angular-devkit/core": "~20.3.13",
    "@angular-devkit/schematics": "~20.3.13",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~21.6.0",
    "@nx/jest": "~21.6.0",
    "@nx/js": "~21.6.0",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@stylistic/eslint-plugin": "~5.6.0",
    "@types/jest": "~30.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "~24.10.0",
    "@typescript-eslint/parser": "~8.52.0",
    "angular-eslint": "~20.6.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.12.0",
    "eslint-plugin-jsdoc": "~54.7.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~60.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~30.2.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~30.2.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~21.6.0",
    "rxjs": "^7.8.1",
    "ts-jest": "~29.4.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.52.0"
  },
  "schematics": "./collection.json",
  "ng-update": {
    "migrations": "./migration.json"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/src/public_api.d.ts",
      "default": "./dist/src/public_api.js"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "main": "./dist/src/public_api.js",
  "types": "./dist/src/public_api.d.ts"
}
