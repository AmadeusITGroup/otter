    "jest-junit": "~16.0.0",
    "jest-preset-angular": "~14.5.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~20.4.0",
    "pid-from-port": "^1.1.3",
    "rxjs": "^7.8.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.2.5",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.7.3",
    "typescript-eslint": "~8.24.0",
    "zone.js": "~0.15.0"
  },
  "engines": {
    "node": "^18.19.1 || ^20.11.1 || >=22.0.0"
  },
  "schematics": "./collection.json",
  "ng-update": {
    "migrations": "./migration.json"
  }
