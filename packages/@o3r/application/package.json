{
  "name": "@o3r/application",
  "version": "0.0.0-placeholder",
  "description": "This module provides development help to monitor your Otter Application",
  "keywords": [
    "application",
    "otter",
    "otter-module"
  ],
  "exports": {
    "./schemas/*": {
      "default": "./schemas/*"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build application",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "prepare:publish": "prepare-publish ./dist"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "~17.0.0",
    "@angular/common": "~17.0.2",
    "@angular/core": "~17.0.2",
    "@o3r/core": "workspace:^",
    "@o3r/logger": "workspace:^",
    "@o3r/routing": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@o3r/testing": "workspace:^",
    "@schematics/angular": "~17.0.0",
    "rxjs": "^7.8.1",
    "typescript": "~5.2.2"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.3",
    "@angular-devkit/core": "~17.0.3",
    "@angular-devkit/schematics": "~17.0.3",
    "@angular-eslint/eslint-plugin": "~17.1.0",
    "@angular/common": "~17.0.4",
    "@angular/compiler": "~17.0.4",
    "@angular/compiler-cli": "~17.0.4",
    "@angular/core": "~17.0.4",
    "@angular/platform-browser": "~17.0.4",
    "@angular/platform-browser-dynamic": "~17.0.4",
    "@babel/core": "~7.23.0",
    "@babel/preset-typescript": "~7.23.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nx/eslint": "~17.1.1",
    "@nx/eslint-plugin": "~17.1.1",
    "@nx/jest": "~17.1.1",
    "@nx/js": "~17.1.1",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/logger": "workspace:^",
    "@o3r/routing": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@o3r/testing": "workspace:^",
    "@schematics/angular": "~17.0.3",
    "@types/jest": "~29.5.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "~27.6.0",
    "eslint-plugin-jsdoc": "~46.9.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-preset-angular": "~13.1.4",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~17.1.1",
    "rxjs": "^7.8.1",
    "typescript": "~5.2.2",
    "zone.js": "~0.14.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "schematics": "./dist/collection.json"
}
