    "@capacitor/device": "^4.0.1",
    "@capacitor/preferences": "^4.0.1",
    "@ngrx/store": "^15.0.0",
    "@nrwl/cli": "^15.3.3",
    "@nrwl/js": "^15.3.3",
    "@nrwl/linter": "^15.3.3",
    "@o3r/build-helpers": "workspace:^",
