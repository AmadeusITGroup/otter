  "description": "This package contains tooling useful for mobile applications.",
  "sideEffects": false,
  "bin": {
    "pcloudy": "./pcloudy/pcloudy.connection.js"
  },
  "exports": {
    "./pcloudy": {
      "default": "./pcloudy/index.js"
    }
  },
