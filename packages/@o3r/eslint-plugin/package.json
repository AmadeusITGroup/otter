{
  "name": "@o3r/eslint-plugin",
  "version": "0.0.0-placeholder",
  "description": "The module provides in-house eslint plugins to use in your own eslint configuration.",
  "main": "dist/src/public_api.js",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "otter",
    "amadeus",
    "linter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build eslint-plugin",
    "postbuild": "patch-package-json-main",
    "prepare:publish": "prepare-publish ./dist",
    "prepare:build:builders": "yarn cpy './collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.3.0",
    "globby": "^11.1.0",
    "semver": "^7.5.2",
    "tslib": "^2.5.3"
  },
  "peerDependenciesMeta": {
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "jsonc-eslint-parser": {
      "optional": true
    },
    "yaml-eslint-parser": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@angular-eslint/template-parser": "~17.1.0",
    "@angular-eslint/utils": "~17.1.0",
    "@angular/compiler": "~17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.22.0",
    "jsonc-eslint-parser": "~2.4.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "~17.0.3",
    "@angular-devkit/schematics": "~17.0.3",
    "@angular-eslint/eslint-plugin": "~17.1.0",
    "@angular-eslint/template-parser": "~17.1.0",
    "@angular-eslint/utils": "~17.1.0",
    "@angular/compiler": "~17.0.4",
    "@babel/core": "~7.23.0",
    "@babel/preset-typescript": "~7.23.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nx/eslint-plugin": "~17.1.1",
    "@nx/jest": "~17.1.1",
    "@o3r/build-helpers": "workspace:^",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "^6.11.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-jest": "~27.6.0",
    "eslint-plugin-jsdoc": "~48.0.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^50.0.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~17.1.1",
    "rimraf": "^5.0.1",
    "ts-jest": "~29.1.1",
    "type-fest": "^4.3.1",
    "typescript": "~5.2.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "schematics": "./collection.json"
}
