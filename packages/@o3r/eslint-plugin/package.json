{
  "name": "@o3r/eslint-plugin",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "The module provides in-house eslint plugins to use in your own eslint configuration.",
  "main": "dist/src/public_api.js",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "otter",
    "amadeus",
    "linter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build eslint-plugin",
    "postbuild": "patch-package-json-main",
    "prepare:publish": "prepare-publish ./dist",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' 'schematics/**/templates/**' dist/schematics && yarn cpy './collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.0.0",
    "globby": "^11.1.0",
    "semver": "^7.5.2",
    "tslib": "^2.6.2",
    "type-fest": "^4.10.2"
  },
  "peerDependenciesMeta": {
    "@angular-eslint/template-parser": {
      "optional": true
    },
    "jsonc-eslint-parser": {
      "optional": true
    },
    "yaml-eslint-parser": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@angular-eslint/template-parser": "~18.3.0",
    "@angular/compiler": "~18.2.0",
    "eslint": "^9.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "~18.2.0",
    "@angular-devkit/schematics": "~18.2.0",
    "@angular-eslint/eslint-plugin": "~18.3.0",
    "@angular-eslint/template-parser": "~18.3.0",
    "@angular-eslint/test-utils": "~18.3.0",
    "@angular/compiler": "~18.2.0",
    "@babel/core": "~7.25.0",
    "@babel/preset-typescript": "~7.24.0",
    "@compodoc/compodoc": "^1.1.19",
    "@eslint/eslintrc": "^3.0.0",
    "@nx/eslint-plugin": "~19.5.0",
    "@nx/jest": "~19.5.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@stylistic/eslint-plugin-ts": "~2.7.0",
    "@types/eslint": "~9.6.0",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/rule-tester": "^8.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-jest": "~28.8.0",
    "eslint-plugin-jsdoc": "~50.2.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~19.5.0",
    "pid-from-port": "^1.1.3",
    "rimraf": "^5.0.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.2.0",
    "type-fest": "^4.10.2",
    "typescript": "~5.5.4",
    "yaml-eslint-parser": "^1.2.2"
  },
  "schematics": "./collection.json"
}
