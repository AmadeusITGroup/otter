{
  "extends": "../../../tsconfig.build",
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "outDir": "./dist",
    "rootDir": ".",
    "tsBuildInfoFile": "./build/.tsbuildinfo"
  },
  "include": [
    "src/**/*.ts",
    "extractors/**/*.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/templates/**/*.ts",
    "**/mocks/**/*.ts"
  ]
}
