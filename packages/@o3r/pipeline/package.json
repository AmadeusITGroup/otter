{
  "name": "@o3r/pipeline",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "typings": "./dist/src/public_api.d.ts",
  "main": "dist/src/public_api.js",
  "description": "A package that provides toolchain related helpers.",
  "keywords": [
    "toolchain",
    "pipeline",
    "otter",
    "otter-module"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "copy:templates": "yarn cpy 'schematics/**/templates{-*,}/**/*' dist/schematics",
    "prepare:build:builders": "yarn cpy '{collection,migration}.json' dist && yarn cpy 'schematics/**/schema.json' dist/schematics && yarn copy:templates",
    "build:source": "tsc -b tsconfig.build.json && yarn cpy package.json dist/",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn copy:templates && yarn generate-cjs-manifest",
    "build": "yarn nx build pipeline",
    "postbuild": "patch-package-json-main"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "typings": "./dist/src/public_api.d.ts",
      "default": "./dist/src/public_api.js"
    }
  },
  "dependencies": {
    "@o3r/schematics": "workspace:^",
    "js-yaml": "^4.1.0",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "^20.0.0",
    "@o3r/schematics": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "type-fest": "^4.30.1"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@o3r/telemetry": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.2002.0",
    "@angular-devkit/core": "~20.2.0",
    "@angular-devkit/schematics": "~20.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~21.5.0",
    "@nx/jest": "~21.5.0",
    "@nx/js": "~21.5.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@schematics/angular": "~20.2.0",
    "@stylistic/eslint-plugin": "~5.3.0",
    "@types/jest": "~29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "~8.42.0",
    "@typescript-eslint/parser": "~8.42.0",
    "@typescript-eslint/utils": "~8.42.0",
    "angular-eslint": "~20.2.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.35.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.0.0",
    "eslint-plugin-jsdoc": "~54.3.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~60.0.0",
    "eslint-plugin-unused-imports": "~4.2.0",
    "globals": "^16.0.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~29.7.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~21.5.0",
    "pid-from-port": "^1.1.3",
    "rxjs": "^7.8.1",
    "ts-jest": "~29.4.0",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.42.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  },
  "schematics": "./collection.json",
  "ng-update": {
    "migrations": "./migration.json"
  },
  "ng-add": {
    "save": "devDependencies"
  }
}
