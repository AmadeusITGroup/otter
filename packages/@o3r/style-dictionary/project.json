{
  "name": "style-dictionary",
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/@o3r/style-dictionary/src",
  "prefix": "o3r",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/dist/package.json"
      ],
      "options": {
        "script": "postbuild"
      },
      "dependsOn": [
        "^build",
        "compile",
        "expose-schemas"
      ]
    },
    "expose-schemas": {},
    "compile": {
      "executor": "nx:run-script",
      "options": {
        "script": "build:source"
      }
    },
    "lint": {},
    "test": {
      "options": {
        "jestConfig": "{projectRoot}/jest.config.cjs"
      }
    },
    "test-int": {
      "options": {
        "jestConfig": "{projectRoot}/testing/jest.config.it.cjs"
      }
    },
    "prepare-build-builders": {},
    "build-builders": {},
    "prepare-publish": {},
    "publish": {},
    "documentation": {}
  },
  "tags": []
}
