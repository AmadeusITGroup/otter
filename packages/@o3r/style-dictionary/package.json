{
  "name": "@o3r/style-dictionary",
  "version": "0.0.0-placeholder",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "typings": "./dist/src/public_api.d.mts",
  "main": "./dist/src/public_api.mjs",
  "description": "Package exposing modules and hooks for Style Dictionary",
  "keywords": [
    "design",
    "otter",
    "otter-module",
    "style-dictionary"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "copy:schemas": "yarn cpy 'schemas/**/*' dist/schemas",
    "copy:templates": "yarn cpy 'schematics/**/templates/**' dist/schematics",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest",
    "build:source": "tsc -b tsconfig.build.json && yarn cpy package.json dist/",
    "build": "yarn nx build design",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' dist/schematics && yarn cpy 'collection.json' dist && yarn copy:templates",
    "postbuild": "patch-package-json-main"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    "./schemas/*.json": {
      "default": "./schemas/*.json"
    },
    ".": {
      "module": "./dist/src/public_api.mjs",
      "typings": "./dist/src/public_api.d.mts",
      "default": "./dist/src/public_api.mjs"
    }
  },
  "dependencies": {
    "@angular-devkit/architect": "~0.1902.0",
    "@angular-devkit/schematics": "~19.2.0",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "~19.2.0",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@o3r/core": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "style-dictionary": "^4.3.2",
    "type-fest": "^4.30.1"
  },
  "peerDependenciesMeta": {
    "@o3r/core": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@o3r/telemetry": {
      "optional": true
    },
    "chokidar": {
      "optional": true
    },
    "globby": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "~7.27.0",
    "@babel/preset-env": "~7.27.0",
    "@babel/preset-typescript": "~7.27.0",
    "@compodoc/compodoc": "^1.1.19",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint": "~20.8.0",
    "@nx/eslint-plugin": "~20.8.0",
    "@nx/jest": "~20.8.0",
    "@nx/js": "~20.8.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/telemetry": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@schematics/angular": "~19.2.0",
    "@stylistic/eslint-plugin": "~3.1.0",
    "@types/jest": "~29.5.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.0.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/parser": "~8.33.0",
    "angular-eslint": "~19.4.0",
    "babel-jest": "^29.7.0",
    "chokidar": "^4.0.3",
    "cpy-cli": "^5.0.0",
    "eslint": "~9.28.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~3.10.0",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~28.12.0",
    "eslint-plugin-jsdoc": "~50.6.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~56.0.0",
    "eslint-plugin-unused-imports": "~4.1.4",
    "globals": "^15.9.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-resolve": "~29.7.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~20.8.0",
    "pid-from-port": "^1.1.3",
    "style-dictionary": "~4.4.0",
    "ts-jest": "~29.3.0",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "~8.33.0",
    "zone.js": "~0.15.0"
  },
  "engines": {
    "node": "^20.11.1 || >=22.0.0"
  },
  "schematics": "./collection.json"
}
