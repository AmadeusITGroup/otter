{
  "name": "@o3r/eslint-config",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Recommended eslint v9+ configuration for Otter project",
  "main": "./src/rules/typescript.cjs",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "default": "./src/rules/typescript.cjs"
    },
    "./helpers": {
      "default": "./src/helpers/index.cjs"
    },
    "./typescript": {
      "default": "./src/rules/typescript.cjs"
    },
    "./template": {
      "default": "./src/rules/template.cjs"
    }
  },
  "keywords": [
    "eslint",
    "otter",
    "amadeus",
    "linter",
    "otter-module"
  ],
  "scripts": {
    "build": "yarn cpy package.json dist && yarn cpy src dist && yarn postbuild",
    "postbuild": "patch-package-json-main",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' 'schematics/**/templates/**' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "dependencies": {
    "@o3r/schematics": "workspace:~"
  },
  "peerDependencies": {
    "@angular-devkit/core": "^21.0.0",
    "@angular-devkit/schematics": "^21.0.0",
    "@angular-eslint/builder": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/js": "^8.57.0 || ^9.22.0",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/schematics": "workspace:~",
    "@schematics/angular": "^21.0.0",
    "@stylistic/eslint-plugin": "~5.6.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typescript-eslint/types": "^8.15.0",
    "@typescript-eslint/utils": "^8.15.0",
    "angular-eslint": "^21.0.0",
    "eslint": "^8.57.0 || ^9.22.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jest": "^29.0.0",
    "eslint-plugin-jsdoc": "~61.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unicorn": "~62.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "globby": "^11.1.0",
    "jest": "^29.0.0 || ^30.2.0",
    "jsonc-eslint-parser": "^2.4.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.15.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "~21.0.2",
    "@angular-devkit/schematics": "~21.0.2",
    "@angular/compiler": "~21.0.3",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/js": "~9.39.0",
    "@nx/eslint-plugin": "~22.3.3",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@schematics/angular": "~21.0.2",
    "@stylistic/eslint-plugin": "~5.6.0",
    "@types/jest": "~30.0.0",
    "@types/node": "~24.10.0",
    "@typescript-eslint/parser": "~8.51.0",
    "angular-eslint": "~21.0.1",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.11.0",
    "eslint-plugin-jsdoc": "~61.5.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~62.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~30.2.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~30.2.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~22.3.3",
    "rxjs": "^7.8.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.4.0",
    "tslib": "^2.6.2",
    "type-fest": "^5.3.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.51.0"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/core": {
      "optional": true
    },
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@angular-eslint/builder": {
      "optional": true
    },
    "@angular/compiler": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@schematics/angular": {
      "optional": true
    },
    "@typescript-eslint/types": {
      "optional": true
    },
    "@typescript-eslint/utils": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "globby": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "yaml-eslint-parser": {
      "optional": true
    }
  },
  "schematics": "./collection.json"
}
