{
  "name": "@o3r/eslint-config",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Recommended eslint v9+ configuration for Otter project",
  "main": "./src/rules/typescript.cjs",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "default": "./src/rules/typescript.cjs"
    },
    "./helpers": {
      "default": "./src/helpers/index.cjs"
    },
    "./typescript": {
      "default": "./src/rules/typescript.cjs"
    },
    "./template": {
      "default": "./src/rules/template.cjs"
    }
  },
  "keywords": [
    "eslint",
    "otter",
    "amadeus",
    "linter",
    "otter-module"
  ],
  "scripts": {
    "build": "yarn cpy package.json dist && yarn cpy src dist && yarn postbuild",
    "postbuild": "patch-package-json-main",
    "prepare:build:builders": "yarn cpy 'schematics/**/*.json' 'schematics/**/templates/**' dist/schematics && yarn cpy 'collection.json' dist",
    "build:builders": "tsc -b tsconfig.builders.json --pretty && yarn generate-cjs-manifest"
  },
  "dependencies": {
    "@o3r/schematics": "workspace:^"
  },
  "peerDependencies": {
    "@angular-devkit/core": "^20.0.0",
    "@angular-devkit/schematics": "^20.0.0",
    "@angular-eslint/builder": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/js": "^8.57.0 || ^9.22.0",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/schematics": "workspace:^",
    "@schematics/angular": "^20.0.0",
    "@stylistic/eslint-plugin": "~5.5.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@typescript-eslint/types": "^8.15.0",
    "@typescript-eslint/utils": "^8.15.0",
    "angular-eslint": "^20.0.0",
    "eslint": "^8.57.0 || ^9.22.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^4.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-jest": "^29.0.0",
    "eslint-plugin-jsdoc": "^54.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "globby": "^11.1.0",
    "jest": "^29.7.0",
    "jsonc-eslint-parser": "^2.4.0",
    "ts-node": "^10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.15.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@angular-devkit/core": "~20.3.0",
    "@angular-devkit/schematics": "~20.3.0",
    "@angular/compiler": "~20.3.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/js": "~9.38.0",
    "@nx/eslint-plugin": "~21.6.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@schematics/angular": "~20.3.0",
    "@stylistic/eslint-plugin": "~5.5.0",
    "@types/jest": "~29.5.2",
    "@types/node": "~22.18.0",
    "@typescript-eslint/parser": "~8.46.0",
    "angular-eslint": "~20.4.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.38.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.0.0",
    "eslint-plugin-jsdoc": "~54.7.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~60.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~29.7.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~21.6.0",
    "pid-from-port": "^1.1.3",
    "rxjs": "^7.8.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.4.0",
    "ts-node": "~10.9.2",
    "tslib": "^2.6.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.46.0"
  },
  "peerDependenciesMeta": {
    "@angular-devkit/core": {
      "optional": true
    },
    "@angular-devkit/schematics": {
      "optional": true
    },
    "@angular-eslint/builder": {
      "optional": true
    },
    "@angular/compiler": {
      "optional": true
    },
    "@o3r/schematics": {
      "optional": true
    },
    "@schematics/angular": {
      "optional": true
    },
    "@typescript-eslint/types": {
      "optional": true
    },
    "@typescript-eslint/utils": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "globby": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "type-fest": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "yaml-eslint-parser": {
      "optional": true
    }
  },
  "schematics": "./collection.json"
}
