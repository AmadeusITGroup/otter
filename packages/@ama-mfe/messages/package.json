{
  "name": "@ama-mfe/messages",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Contains common messages types used for iframes dialog in mfe architecture",
  "module": "dist/src/public_api.js",
  "types": "dist/src/public_api.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "module": "./dist/src/public_api.js",
      "types": "./dist/src/public_api.d.ts",
      "node": "./dist/cjs/public_api.js",
      "require": "./dist/cjs/public_api.js",
      "default": "./dist/cjs/public_api.js"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-mfe-messages",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs -q --strip-leading-paths",
    "build:esm2020": "tsc -b tsconfig.build.json",
    "postbuild": "yarn cpy './package.json' dist && patch-package-json-main"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@amadeus-it-group/microfrontends": "0.0.10"
  },
  "peerDependenciesMeta": {
    "@amadeus-it-group/microfrontends": {
      "optional": true
    }
  },
  "devDependencies": {
    "@amadeus-it-group/microfrontends": "0.0.10",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~21.6.0",
    "@nx/jest": "~21.6.0",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@schematics/angular": "~20.3.13",
    "@stylistic/eslint-plugin": "~5.6.0",
    "@swc/cli": "~0.7.9",
    "@swc/core": "~1.15.3",
    "@types/jest": "~30.0.0",
    "@types/node": "~24.10.0",
    "@typescript-eslint/parser": "~8.50.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.10.0",
    "eslint-plugin-jsdoc": "~54.7.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~60.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "jest": "~30.2.0",
    "jest-junit": "~16.0.0",
    "jest-util": "~30.2.0",
    "jsonc-eslint-parser": "~2.4.0",
    "minimist": "^1.2.6",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.4.0",
    "type-fest": "^4.30.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.50.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  }
}
