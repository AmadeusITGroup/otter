{
  "name": "@ama-mfe/messages",
  "version": "0.0.0-placeholder",
  "publishConfig": {
    "access": "public"
  },
  "description": "Contains common messages types used for iframes dialog in mfe architecture",
  "module": "dist/src/public_api.js",
  "typings": "dist/src/public_api.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "module": "./dist/src/public_api.js",
      "typings": "./dist/src/public_api.d.ts",
      "node": "./dist/cjs/public_api.js",
      "require": "./dist/cjs/public_api.js",
      "default": "./dist/cjs/public_api.js"
    }
  },
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build": "yarn nx build ama-mfe-messages",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs -q --strip-leading-paths",
    "build:esm2020": "tsc -b tsconfig.build.json",
    "postbuild": "yarn cpy './package.json' dist && patch-package-json-main"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@amadeus-it-group/microfrontends": "0.0.4"
  },
  "peerDependenciesMeta": {
    "@amadeus-it-group/microfrontends": {
      "optional": true
    }
  },
  "devDependencies": {
    "@amadeus-it-group/microfrontends": "0.0.4",
    "@compodoc/compodoc": "^1.1.19",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint-plugin": "~20.6.0",
    "@nx/jest": "~20.6.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/test-helpers": "workspace:^",
    "@schematics/angular": "~19.2.0",
    "@stylistic/eslint-plugin": "~3.1.0",
    "@stylistic/eslint-plugin-ts": "~3.1.0",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.11.0",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "~8.29.0",
    "cpy-cli": "^5.0.0",
    "eslint": "~9.23.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~3.10.0",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~28.11.0",
    "eslint-plugin-jsdoc": "~50.6.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~56.0.0",
    "eslint-plugin-unused-imports": "~4.1.4",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jsonc-eslint-parser": "~2.4.0",
    "minimist": "^1.2.6",
    "pid-from-port": "^1.1.3",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "semver": "^7.5.2",
    "ts-jest": "~29.3.0",
    "ts-node": "~10.9.2",
    "type-fest": "^4.30.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "~8.29.0"
  },
  "engines": {
    "node": "^20.11.1 || >=22.0.0"
  }
}
