{
  "name": "@ama-openapi/cli",
  "version": "0.0.0-placeholder",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "description": "CLI to manage OpenAPI models",
  "keywords": [
    "otter",
    "otter-module",
    "openapi"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "build:source": "tsc -b tsconfig.build.json && yarn cpy package.json dist/",
    "build": "yarn nx build ama-openapi-cli",
    "postbuild": "patch-package-json-main"
  },
  "bin": {
    "ama-openapi": "./src/main.mjs"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "module": "./src/main.mjs",
      "types": "./src/main.d.mts",
      "default": "./src/main.mjs"
    }
  },
  "dependencies": {
    "@ama-openapi/core": "workspace:~",
    "tslib": "^2.6.2",
    "yargs": "~18.0.0"
  },
  "peerDependencies": {
    "@o3r/telemetry": "workspace:~",
    "chokidar": "^4.0.3"
  },
  "peerDependenciesMeta": {
    "@o3r/telemetry": {
      "optional": true
    },
    "chokidar": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "~7.28.0",
    "@babel/preset-env": "~7.28.0",
    "@babel/preset-typescript": "~7.28.0",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint": "~22.3.3",
    "@nx/eslint-plugin": "~22.3.3",
    "@nx/jest": "~22.3.3",
    "@nx/js": "~22.3.3",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/telemetry": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@stylistic/eslint-plugin": "~5.6.0",
    "@types/jest": "~30.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/minimist": "^1.2.2",
    "@types/node": "~24.10.0",
    "@types/semver": "^7.3.13",
    "@types/yargs": "~17.0.33",
    "@typescript-eslint/parser": "~8.51.0",
    "angular-eslint": "~21.0.1",
    "babel-jest": "^30.0.5",
    "chokidar": "^4.0.3",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.11.0",
    "eslint-plugin-jsdoc": "~61.5.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~62.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~30.2.0",
    "jest-junit": "~16.0.0",
    "jest-resolve": "~30.2.0",
    "jest-util": "~30.2.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~22.3.3",
    "ts-jest": "~29.4.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.51.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  }
}
