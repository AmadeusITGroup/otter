{
  "name": "@ama-openapi/core",
  "version": "0.0.0-placeholder",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "types": "./src/public_api.d.mts",
  "main": "./src/public_api.mjs",
  "description": "Package exposing core function to manage OpenAPI dependencies",
  "keywords": [
    "otter",
    "otter-module",
    "openapi"
  ],
  "scripts": {
    "nx": "nx",
    "ng": "yarn nx",
    "copy:schemas": "yarn cpy 'schemas/*.json' dist/schemas",
    "copy:renovate": "yarn cpy 'renovate/*.json' dist/renovate",
    "build:source": "tsc -b tsconfig.build.json && yarn cpy package.json dist/",
    "build": "yarn nx build ama-openapi-core",
    "postbuild": "patch-package-json-main"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    "./schemas/*.json": {
      "default": "./schemas/*.json"
    },
    "./renovate/*.json": {
      "default": "./renovate/*.json"
    },
    ".": {
      "module": "./src/public_api.mjs",
      "types": "./src/public_api.d.mts",
      "default": "./src/public_api.mjs"
    }
  },
  "dependencies": {
    "ajv": "~8.17.1",
    "js-yaml": "^4.1.1",
    "semver": "^7.5.2",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "type-fest": "^4.30.1"
  },
  "peerDependenciesMeta": {
    "type-fest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "~7.28.0",
    "@babel/preset-env": "~7.28.0",
    "@babel/preset-typescript": "~7.28.0",
    "@compodoc/compodoc": "^1.1.19",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@nx/eslint": "~21.6.0",
    "@nx/eslint-plugin": "~21.6.0",
    "@nx/jest": "~21.6.0",
    "@nx/js": "~21.6.0",
    "@o3r/build-helpers": "workspace:~",
    "@o3r/eslint-plugin": "workspace:~",
    "@o3r/telemetry": "workspace:~",
    "@o3r/test-helpers": "workspace:~",
    "@stylistic/eslint-plugin": "~5.5.0",
    "@types/jest": "~29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/minimist": "^1.2.2",
    "@types/node": "~22.18.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/parser": "~8.46.0",
    "angular-eslint": "~20.6.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.0.0",
    "cpy-cli": "^6.0.0",
    "eslint": "~9.39.0",
    "eslint-import-resolver-node": "~0.3.9",
    "eslint-import-resolver-typescript": "~4.4.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-import-newlines": "~1.4.0",
    "eslint-plugin-jest": "~29.0.0",
    "eslint-plugin-jsdoc": "~54.7.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-unicorn": "~60.0.0",
    "eslint-plugin-unused-imports": "~4.3.0",
    "globals": "^16.0.0",
    "jest": "~29.7.0",
    "jest-junit": "~16.0.0",
    "jest-resolve": "~29.7.0",
    "jest-util": "~29.7.0",
    "jsonc-eslint-parser": "~2.4.0",
    "nx": "~21.6.0",
    "ts-jest": "~29.4.0",
    "type-fest": "^4.30.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "~8.46.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.17.0 || ^24.0.0"
  }
}
