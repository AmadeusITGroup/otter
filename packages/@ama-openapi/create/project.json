{
  "name": "ama-openapi-create",
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/@ama-openapi/create/src",
  "prefix": "o3r",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/dist/package.json"
      ],
      "options": {
        "script": "postbuild"
      },
      "dependsOn": [
        "compile",
        "copy-templates"
      ]
    },
    "copy-templates": {
      "executor": "nx:run-script",
      "outputs": [
        "{projectRoot}/dist/templates/**"
      ],
      "inputs": [
        "templates"
      ],
      "options": {
        "script": "copy:templates"
      },
      "dependsOn": [
        "compile"
      ]
    },
    "compile": {
      "executor": "nx:run-script",
      "outputs": ["{projectRoot}/dist/index.*"],
      "options": {
        "script": "compile"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "set-version": {},
    "prepare-publish": {},
    "publish": {}
  },
  "tags": [
    "scope:ama-openapi",
    "type:create"
  ]
}
