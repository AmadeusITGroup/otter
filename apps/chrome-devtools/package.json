{
  "name": "@o3r/chrome-devtools",
  "version": "0.0.0-placeholder",
  "description": "Otter Chrome Devtools",
  "main": "dist/extension/core.js",
  "types": "dist/extension/core.d.ts",
  "keywords": [
    "otter",
    "amadeus"
  ],
  "scripts": {
    "build": "yarn nx build chrome-devtools",
    "postbuild:patch": "yarn patch:manifest && yarn patch:extension && yarn patch:package",
    "patch:package": "cpy 'package.json' 'dist' && patch-package-json-main",
    "copy:assets": "cpy 'src/manifest.json' 'src/devtools.html' dist --flat && cpy './src/assets/**' dist",
    "patch:manifest": "node scripts/sanitize-manifest.cjs",
    "build:extension": "tsc -b tsconfig.extension.json",
    "patch:extension": "node scripts/sanitize-extension.cjs",
    "start": "concurrently 'yarn:watch:*'",
    "watch:panel": "yarn build:panel --watch",
    "watch:components": "yarn build:components --watch",
    "watch:manifest": "yarn patch:manifest --watch",
    "watch:extension": "tsc-watch -b tsconfig.extension.json --noClear --onSuccess 'yarn patch:extension'",
    "prepare:publish": "prepare-publish ./dist && node scripts/set-manifest-version.cjs",
    "publish-to-market": "node ./scripts/publish-to-market.mjs"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.1.6",
    "@angular-eslint/builder": "~16.1.0",
    "@angular-eslint/eslint-plugin": "~16.1.0",
    "@angular-eslint/eslint-plugin-template": "~16.1.0",
    "@angular-eslint/template-parser": "~16.1.0",
    "@angular/cli": "~16.1.6",
    "@angular/compiler": "~16.1.6",
    "@angular/compiler-cli": "~16.1.6",
    "@angular/platform-browser": "~16.1.6",
    "@angular/platform-browser-dynamic": "~16.1.6",
    "@nx/jest": "~16.6.0",
    "@o3r/build-helpers": "workspace:^",
    "@o3r/dev-tools": "workspace:^",
    "@o3r/eslint-config-otter": "workspace:^",
    "@o3r/eslint-plugin": "workspace:^",
    "@o3r/localization": "workspace:^",
    "@o3r/rules-engine": "workspace:^",
    "@types/chrome": "^0.0.242",
    "@types/jest": "~29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@typescript-eslint/types": "^5.59.9",
    "archiver": "^5.3.1",
    "chokidar": "^3.5.2",
    "chrome-webstore-upload": "^1.0.0",
    "concurrently": "^8.0.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^47.0.0",
    "globby": "^11.1.0",
    "jest": "~29.6.0",
    "jest-environment-jsdom": "~29.6.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^13.1.1",
    "minimist": "^1.2.6",
    "nx": "~16.6.0",
    "rimraf": "^5.0.1",
    "tsc-watch": "^6.0.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@angular/animations": "~16.1.6",
    "@angular/common": "~16.1.6",
    "@angular/core": "~16.1.6",
    "@angular/forms": "~16.1.6",
    "@angular/localize": "~16.1.6",
    "@angular/router": "~16.1.6",
    "@ng-bootstrap/ng-bootstrap": "^15.0.1",
    "@ngrx/entity": "~16.1.0",
    "@ngrx/store": "~16.1.0",
    "@o3r/application": "workspace:^",
    "@o3r/components": "workspace:^",
    "@o3r/configuration": "workspace:^",
    "@o3r/core": "workspace:^",
    "@o3r/localization": "workspace:^",
    "@o3r/rules-engine": "workspace:^",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "~0.13.1"
  }
}
