    "@o3r/schematics": "workspace:^",
    "@o3r/testing": "workspace:^",
    "@playwright/test": "~1.42.0",
    "@schematics/angular": "~17.3.0",
    "@stylistic/eslint-plugin-ts": "^1.5.4",
