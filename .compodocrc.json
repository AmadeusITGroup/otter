{
  "$schema": "https://raw.githubusercontent.com/compodoc/compodoc/develop/src/config/schema.json",
  "name": "Otter Library",
  "output": "./generated-doc/",
  "tsconfig": "./tsconfig.doc.json",
  "includes": "./docs/",
  "includesName": "General",
  "assetsFolder": "./.attachments",
  "theme": "material",
  "maxSearchResults": 0,
  "disableLifeCycleHooks": true,
  "disablePrivate": true,
  "disableProtected": true,
  "disableInternal": true,
  "disableCoverage": true,
  "hideGenerator": true,
  "templates": "./compodoc-templates"
}
